@font-face {
  font-family: 'iconfont';  /* project id 1230759 */
  src: url('//at.alicdn.com/t/font_1230759_pkv8rn8yy7.eot');
  src: url('//at.alicdn.com/t/font_1230759_pkv8rn8yy7.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1230759_pkv8rn8yy7.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1230759_pkv8rn8yy7.woff') format('woff'),
  url('//at.alicdn.com/t/font_1230759_pkv8rn8yy7.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1230759_pkv8rn8yy7.svg#iconfont') format('svg');
