.geek-icon {
    font: normal normal normal 16px "iconfont";
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;

    &-main::before {
        content: "\e615";
    }

    &-setting::before {
        content: "\e608";
    }

    &-menu::before {
        content: "\e729";
    }
    &-open::before {
        content: "\e648";
    }
    &-close::before {
        content: "\e647";
    }
}